{
    "alignment.indentBase" : "firstline",
    "alignment.operatorPadding" : "right",
    "alignment.surroundSpace" : {
        "colon"      : [0, 1], // The first number specify how much space to add to the left, can be negative.
                               // The second number is how much space to the right, can be negative.
        "assignment" : [1, 1], // The same as above.
        "arrow"      : [1, 1], // The same as above.
        "comment"    : 2       // Special how much space to add between the trailing comment and the code.
                               // If this value is negative, it means don't align the trailing comment.
    },
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolSortOrder": "name",
    "city-lights-icons-vsc.hidesExplorerArrows": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.autoIndent": "full",
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 1,
    "editor.detectIndentation": false,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "'Noto Mono'",
    "editor.fontFamily": "'Hasklig ExtraLight'",
    "editor.fontFamily": "'Hasklig Light'",
    "editor.fontFamily": "'Hasklig Medium'",
    "editor.fontFamily": "'Hasklig SemiBold'",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.lineHeight": 16,
    "editor.fontWeight": "100",
    "editor.lineNumbers": "interval",
    "editor.minimap.enabled": true,
    "editor.mouseWheelScrollSensitivity": 5,
    "editor.mouseWheelZoom": true,
    "editor.multiCursorModifier": "alt",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [109],
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",<>/?",
    "editor.wordWrap": "on",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "open-in-vim.openMethod": "integrated-terminal",
    "plantuml.previewFileType": "svg",
    "plantuml.exportFormat": "png",
    "plantuml.exportOutDir": ".",
    "plantuml.previewSnapIndicators": true,
    "plantuml.server": "https://www.plantuml.com/plantuml.disabled",
    "search.showLineNumbers": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.env.linux": {},
    "terminal.integrated.experimentalBufferImpl": "TypedArray",
    "terminal.integrated.splitCwd": "inherited",
    "workbench.colorTheme": "Gotham",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.guides.indentation": true,
    "redhat.telemetry.enabled": false,
    "VimTer.vimPath": "/usr/bin/vim",
    "vscode-neovim.neovimExecutablePaths.linux": "/snap/bin/nvim",
    "workbench.editor.empty.hint": "hidden",
    "git.confirmSync": false,
    "git.autofetch": true,
    "markdownlint.config": {
        "no-bare-urls": false,
        "no-multiple-blanks": false,
        "MD013": {
            "line_length": 109
        },
        "blanks-around-headings": false,
        "blanks-around-fences": false,
        "single-h1": false,
        "no-space-in-emphasis": false,
        "no-missing-space-atx": false,
        "fenced-code-language": false
    }
}
